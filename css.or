* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

body {
    font-family: 'Arial', sans-serif;
    line-height: 1.6;
    background-color: #f4f4f9;
    color: #333;
}


header {
    background-image: url('data:image/jpeg;base64,');
    background-size: cover; 
    background-position: center; 
    background-attachment: fixed; 
    color: #fff;
    text-align: center;
    padding: 100px 20px;
}

header h1 {
    font-size: 3rem;
    margin-bottom: 10px;
}

.slogan {
    font-size: 1.2rem;
    font-style: italic;
    margin-top: 10px;
}


.main-content {
    padding: 40px 20px;
    background-color: #fff;
    text-align: center;
}

.main-content h2 {
    font-size: 2.5rem;
    margin-bottom: 20px;
    color: #1e3a8a;
}

.main-content p {
    font-size: 1.1rem;
    line-height: 1.8;
    max-width: 800px;
    margin: 0 auto;
}


.highlights {
    padding: 40px 20px;
    background-color: #eef2f7;
    text-align: center;
}

.highlights h2 {
    font-size: 2.5rem;
    color: #1e3a8a;
    margin-bottom: 30px;
}

.cards {
    display: flex;
    justify-content: center;
    gap: 30px;
    flex-wrap: wrap;
}

.card {
    background-color: #fff;
    padding: 20px;
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    max-width: 300px;
    text-align: center;
    transition: transform 0.3s ease-in-out;
}

.card:hover {
    transform: scale(1.05);
}

.card h3 {
    font-size: 1.8rem;
    color: #1e3a8a;
    margin-bottom: 10px;
}

.card p {
    font-size: 1rem;
    color: #666;
}


footer {
    background-color: #1e3a8a;
    color: #fff;
    text-align: center;
    padding: 20px;
}

footer p {
    font-size: 1rem;
}
